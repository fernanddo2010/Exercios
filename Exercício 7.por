programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite um numero de 1 a 10: ")
    leia(numero)
    se(numero<= 2)
    escreva("Poção de Vida")
    senao se(numero<= 5)
    escreva("Moedas de Ouro")
    senao se(numero<= 8)
    escreva("espada Comum")
    senao se(numero<= 10) 
    escreva("Item Lendario")
    senao
    escreva("Numero invalido")
 }
}
