programa {
  funcao inicio() {
  cadeia caminho
  escreva("Escolha um caminho(esquerda ou direita): ")  
  leia(caminho)
  se(caminho=="esquerda")
  escreva("Você encontrou um tesouro!")
  senao se(caminho=="direita")
  escreva("Você caiu em uma armadilha!")
  senao
  escreva("Escolha invalida. Tente novamente.")
  }
}
